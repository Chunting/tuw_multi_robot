<launch>
	<arg name="cfg"  default="default" />
	<arg name="use_gdb"  default="false" />
	
    <group if="$(arg use_gdb)">
	<node launch-prefix="gdb -ex run --args" pkg="tuw_global_planner" type="tuw_global_planner_node" name="tuw_global_planner" output="screen">
		<rosparam command="load" file="$(find tuw_global_planner)/cfg/multi_robot_planner/$(arg cfg)/multi_robot.yaml" />
	</node>
    </group>
    <group unless="$(arg use_gdb)">
	<node pkg="tuw_global_planner" type="tuw_global_planner_node" name="tuw_global_planner" output="screen">
		<rosparam command="load" file="$(find tuw_global_planner)/cfg/multi_robot_planner/$(arg cfg)/multi_robot.yaml" />
	</node>
    </group>
</launch>

	
