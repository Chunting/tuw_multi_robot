<library path="lib/libtuw_multi_robot_rviz">
  <class name="tuw_voronoi_graph"
         type="tuw_multi_robot_rviz::VoronoiGraphDisplay"
         base_class_type="rviz::Display">
    <description>
      Displays Voronoi Graphs in RVIZ
    </description>
    <message_type>voronoi_segmentation/VoronoiGraph</message_type>
  </class>
  <class name="tuw_voronoi_segment"
         type="tuw_multi_robot_rviz::VoronoiSegmentDisplay"
         base_class_type="rviz::Display">
    <description>
      Displays Voronoi Segments in RVIZ
    </description>
    <message_type>voronoi_segmentation/VoronoiGraph</message_type>
  </class>
  <class name="Goal Selector"
         type="tuw_multi_robot_rviz::MultiRobotGoalSelector"
         base_class_type="rviz::Tool">
    <description>
      Tool for planting flags on the ground plane in rviz.
    </description>
  </class>
</library>