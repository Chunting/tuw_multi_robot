<launch>
  <node pkg="tuw_voronoi_graph" type="tuw_voronoi_graph_generator_node" name="graph_generator" output="screen">
    <param name="graph_cache_path" value="$(find tuw_voronoi_graph)/cfg/graph/cave/cache/" />
    <rosparam command="load" file="$(find tuw_voronoi_graph)/cfg/graph/cave/graph.yaml"/>
  </node>

  <include file="$(find tuw_voronoi_graph)/launch/map_server.launch">
    <arg name="room" value="cave" />
  </include>

  <test name="publishtest" test-name="publishtest"  pkg="rostest" type="publishtest" time-limit="20.0">
    <rosparam>
      topics:
        - name: /segments
          timeout: 10
          negative: True
    </rosparam>
  </test>
</launch>